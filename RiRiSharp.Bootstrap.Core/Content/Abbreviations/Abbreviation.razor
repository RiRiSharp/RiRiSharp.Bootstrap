@inherits BootstrapCoreChildContentComponent

<span @attributes="AdditionalAttributes" title="@(FullName)" class="@(Type.ToBootstrapClass()) @(Classes)">
    @(ChildContent)
</span>


@code {
    [Parameter, EditorRequired] public string FullName { get; set; }
    [Parameter] public AbbreviationType Type { get; set; }
}